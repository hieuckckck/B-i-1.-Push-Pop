#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;
    cin>>n;
    vector<pair<int,pair<int,int>>>a(n);
    for(int i=0;i<n;i++){
        cin>>a[i].first>>a[i].second.first>>a[i].second.second;
        cout<<a[i].first+a[i].second.first+a[i].second.second<<" ";
    }
    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
